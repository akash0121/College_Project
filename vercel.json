{
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/auth/(.*)",
        "dest": "/auth/$1"
      },
      {
        "src": "/decodeDetails",
        "dest": "/decodeDetails"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  